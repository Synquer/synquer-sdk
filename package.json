{
  "name": "synquer-sdk",
  "version": "0.0.0-dev.1",
  "description": "Lightweight observability SDK for Synquer platform",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sync",
    "observability",
    "telemetry",
    "monitoring",
    "integration",
    "jobs"
  ],
  "author": "Synquer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Synquer/synquer-sdk"
  },
  "homepage": "https://synquer.dev",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "msw": "^2.7.0",
    "postgres": "^3.4.8",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
